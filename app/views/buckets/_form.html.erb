<%= form_with(model: @document , url:project_bucket_documents_path(@project), data:{ upload_file_target: "upload_form" }, class: 'hidden flex max-w-max px-4 py-6 rounded-sm items-center absolute top-0 right-0 shadow bg-theme-50 z-10') do |f| %>
  <%= f.file_field :document %>
  <%= f.submit 'Upload', data: { action:"click->upload_file#submit", disable_with:"Upload file...."} , class:"p-2 rounded-md theme-fill-btn"%>
<% end %>
